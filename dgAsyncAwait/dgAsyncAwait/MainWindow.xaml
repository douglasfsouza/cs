<Window x:Class="dgAsyncAwait.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dgAsyncAwait"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnSync" Content="Sync" HorizontalAlignment="Left" Margin="317,103,0,0" VerticalAlignment="Top" Click="btnSync_Click"/>
        <Button x:Name="btnAsync" Content="Async" HorizontalAlignment="Center" Margin="0,103,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.153,-1.992" Click="btnAsync_Click"/>
        <Label x:Name="lblResult" Content="Label" HorizontalAlignment="Center" Margin="0,160,0,0" VerticalAlignment="Top" FontSize="20" Width="780"/>

    </Grid>
</Window>
